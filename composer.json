{
    "config": {
        "vendor-dir": "./vendor/"
    },
    "name": "bingo-soft/graphp",
    "description": "Free PHP library that provides mathematical graph-theory objects and algorithms",
    "keywords": ["graphp", "graph", "math"],
    "authors": [{
        "name": "Bingo-Soft",
        "email": "bingosoft@bk.ru"
    }],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "bingo-soft/heap": "^1.7"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {            
            "Graphp\\": "src"
        }    
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    }
}