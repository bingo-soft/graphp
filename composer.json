{
    "config": {
        "vendor-dir": "./vendor/"
    },
    "name": "bingo-soft/graphp",
    "description": "Free PHP library that provides mathematical graph-theory objects and algorithms",
    "keywords": ["graphp", "graph", "math"],
    "authors": [{
        "name": "Bingo-Soft",
        "email": "bingosoft@bk.ru"
    }],
    "license": "LGPL-2.1-only",
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "doctrine/coding-standard": "^6.0",
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "psr-4": {
            "graphp\\": "src"
        }    
    },
    "autoload-dev": {
        "psr-4": {
            "tests\\": "tests"
        }
    }
}